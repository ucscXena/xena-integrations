{
  "name": "xena-integrations",
  "version": "1.2.0",
  "description": "components for embedding UCSC Xena in other apps",
  "main": "main.js",
  "scripts": {
    "test": "webpack-dev-server --config webpack.test.js --devtool eval --port 8081",
    "lint": "eslint --no-color js/*.js test/*.js",
    "eslint": "eslint",
    "clean": "rimraf build/*",
    "clean:dist": "rimraf dist/*",
    "start": "npm run start:hot",
    "start:cold": "webpack-dev-server --devtool eval --progress --colors --history-api-fallback",
    "start:hot": "node ./server-hot.js",
    "build": "webpack",
    "build:prod": "npm run build -- --config webpack.prod.js --devtool source-map",
    "prebuild": "npm run clean",
    "dist": "npm run clean:dist && mkdir -p dist && cp -r js/* dist && NODE_ENV=production babel -d dist js",
    "prepublish": "npm run dist"
  },
  "files": [
    "NOTICE",
    "LICENSE",
    "dist"
  ],
  "author": "Brian Craft",
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.9.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "exports-loader": "^0.6.2",
    "file-loader": "^0.8.1",
    "ghooks": "^1.2.1",
    "html-webpack-plugin": "1.1.0",
    "imports-loader": "^0.6.3",
    "jsverify": "^0.7.1",
    "mocha": "^2.2.1",
    "mocha-loader": "^0.7.1",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.1",
    "redbox-react": "^1.2.0",
    "rimraf": "^2.3.2",
    "semver": "^5.1.0",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "1.7.3",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.4",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.2",
    "react-dom": "^0.14.7",
    "rx": "^3.0.0",
    "rx-dom": "^4.0.4",
    "rx-react": "^0.2.0",
    "underscore": "^1.7.0"
  }
}
